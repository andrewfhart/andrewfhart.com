/**
 * Publications
 **/

h4
  background-color #ccc
  padding 7px 5px
  margin-bottom 5px
  border-radius 5px
  color #333
  
h5
  margin 10px 0px
  border-bottom solid 1px #9baa83
  
.box.pubs p
  text-indent 0px

.box.pubs img
  float left
  border solid 1px #ccc
  padding 5px
  width 75px
  margin-bottom 10px
  margin-right 10px
  
.box.pubs strong
  background-color #ddee88
  font-style normal
  font-weight:normal
